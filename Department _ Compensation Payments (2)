.form-label { margin-bottom: 0.5rem; }

.col-form-label { padding-top: calc(.375rem + var(--bs-border-width)); padding-bottom: calc(.375rem + var(--bs-border-width)); margin-bottom: 0px; font-size: inherit; line-height: 1.5; }

